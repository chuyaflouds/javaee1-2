       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="homework" class="org.example.javaee.class01.model.Homework"></bean>
    <bean id="student" class="org.example.javaee.class01.model.Student"></bean>
    <bean id="studenthomework" class="org.example.javaee.class01.model.StudentHomework"></bean>
    <bean id="addhomework" class="org.example.javaee.class01.jdbc.Homeworkjdbc"></bean>
    <bean id="addstudent" class="org.example.javaee.class01.jdbc.Studentjdbc"></bean>
    <bean id="addstudenthomework" class="org.example.javaee.class01.jdbc.StudentHomeworkjdbc"></bean>

</beans>
